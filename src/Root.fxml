                                    </VBox>
                                    <VBox alignment="CENTER_LEFT" prefHeight="43.0" prefWidth="50.0">
                                       <children>
                                          <TextField fx:id="tf_problemsDisappearTime" minWidth="-Infinity" prefHeight="25.0" prefWidth="50.0" />
                                          <Label text="time" textFill="#9a9a9a" />
                                       </children>
                                    </VBox>
                                 </children>
                              </HBox>
                           </children>
