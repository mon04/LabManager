                              <Insets top="8.0" />
                           </padding>
                        </Separator>
                        <VBox maxWidth="-Infinity" prefHeight="154.0" prefWidth="400.0" spacing="8.0">
                           <children>
                              <HBox alignment="BASELINE_CENTER" prefWidth="200.0" spacing="4.0">
