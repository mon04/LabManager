                        <MenuItem mnemonicParsing="false" text="Source" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="4.0" VBox.vgrow="ALWAYS">
