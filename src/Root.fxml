<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RootController">
   <children>
      <VBox alignment="CENTER_LEFT" layoutX="169.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0" spacing="4.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Saved Modules" textFill="#9a9a9a">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <ListView fx:id="list_savedModules" onKeyPressed="#moduleListKeyPressed" onMouseClicked="#moduleListClicked" prefHeight="153.0" prefWidth="412.0" />
            <Separator prefWidth="200.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets top="8.0" />
               </VBox.margin>
            </Separator>
            <VBox fx:id="vbox_newModule" prefHeight="116.0" prefWidth="372.0" spacing="8.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label fx:id="label_moduleEditorHeading" text="New module" textFill="#9a9a9a">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="4.0">
                     <children>
                        <Label minWidth="-Infinity" prefWidth="45.0" text="Code:" />
                        <TextField fx:id="tf_moduleCode" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="34.0" prefWidth="200.0" spacing="4.0">
                     <children>
                        <Label minWidth="-Infinity" prefWidth="45.0" text="Title:" />
                        <TextField fx:id="tf_moduleTitle" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="63.0" prefWidth="200.0" spacing="4.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label minWidth="-Infinity" prefWidth="45.0" text="Weeks:" />
                        <TableView fx:id="table_weeks" focusTraversable="false" prefHeight="160.0" prefWidth="308.0" HBox.hgrow="ALWAYS">
                           <columns>
                              <TableColumn prefWidth="95.33330285549164" text="Beginning" />
                              <TableColumn prefWidth="211.33333587646484" text="Problem sets" />
                           </columns>
                        </TableView>
                        <Button fx:id="btn_moduleWeeksEdit" mnemonicParsing="false" onAction="#openWeeksEditor" prefWidth="50.0" text="Edit" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="68.0" prefWidth="200.0" spacing="4.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label minWidth="-Infinity" prefWidth="45.0" text="Groups:" />
                        <TableView fx:id="table_groups" focusTraversable="false" prefHeight="160.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                          <columns>
                            <TableColumn prefWidth="95.33330285549164" text="Lab day" />
                            <TableColumn prefWidth="77.00000762939453" text="Lab time" />
                              <TableColumn prefWidth="135.0" text="Group name" />
                          </columns>
                        </TableView>
                        <Button fx:id="btn_moduleGroupsEdit" mnemonicParsing="false" onAction="#openGroupsEditor" prefWidth="50.0" text="Edit" />
                     </children>
                  </HBox>
                  <Separator prefWidth="200.0">
                     <padding>
                        <Insets top="8.0" />
                     </padding>
                  </Separator>
                  <HBox alignment="BASELINE_CENTER" layoutX="10.0" layoutY="36.0" prefWidth="200.0" spacing="4.0">
                     <children>
                        <Label minWidth="-Infinity" prefHeight="17.0" prefWidth="110.0" text="Problems released:" HBox.hgrow="ALWAYS" />
                        <VBox alignment="CENTER_LEFT" prefWidth="189.0" HBox.hgrow="ALWAYS">
                           <children>
                              <ComboBox fx:id="combo_problemsReleasedDay" editable="true" minWidth="100.0" onKeyPressed="#comboProblemsReleasedDayKeyPressed" prefHeight="25.0" prefWidth="1200.0" />
                              <Label text="day of week" textFill="#9a9a9a" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" prefHeight="43.0" prefWidth="50.0">
                           <children>
                              <TextField fx:id="tf_problemsReleasedTime" minWidth="-Infinity" prefHeight="25.0" prefWidth="50.0" />
                              <Label text="time" textFill="#9a9a9a" />
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets top="8.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="TOP_CENTER" layoutX="10.0" layoutY="309.0" prefWidth="200.0" spacing="4.0">
                     <children>
                        <Label minWidth="-Infinity" prefHeight="17.0" prefWidth="110.0" text="CA evaluation ends:" HBox.hgrow="ALWAYS" />
                        <VBox alignment="CENTER_LEFT" prefWidth="189.0" HBox.hgrow="ALWAYS">
                           <children>
                              <ComboBox fx:id="combo_caEvaluationEndsDay" editable="true" minWidth="100.0" onKeyPressed="#comboCaEvaluationEndsDayKeyPressed" prefHeight="25.0" prefWidth="1200.0" />
                              <Label text="day of week" textFill="#9a9a9a" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" minWidth="-Infinity" prefHeight="43.0" prefWidth="50.0">
                           <children>
                              <TextField fx:id="tf_caEvaluationEndsTime" minWidth="-Infinity" prefHeight="25.0" prefWidth="50.0" />
                              <Label text="time" textFill="#9a9a9a" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox alignment="BASELINE_LEFT" layoutX="10.0" layoutY="335.0" prefWidth="200.0" spacing="4.0">
                     <children>
                        <Label minWidth="-Infinity" prefHeight="17.0" prefWidth="110.0" text="Problems disappear:" HBox.hgrow="ALWAYS" />
                        <VBox alignment="CENTER_LEFT" prefWidth="189.0" HBox.hgrow="ALWAYS">
                           <children>
                              <DatePicker fx:id="date_problemsDisappear" editable="false" minWidth="100.0" prefHeight="25.0" prefWidth="1200.0" />
                              <Label text="date" textFill="#9a9a9a" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" prefHeight="43.0" prefWidth="50.0">
                           <children>
                              <TextField fx:id="tf_problemsDisappearTime" minWidth="-Infinity" prefHeight="25.0" prefWidth="50.0" />
                              <Label text="time" textFill="#9a9a9a" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets top="8.0" />
               </VBox.margin>
            </VBox>
            <HBox alignment="BOTTOM_RIGHT" prefWidth="200.0" spacing="8.0">
               <children>
                  <Button fx:id="btn_deleteModule" mnemonicParsing="false" onAction="#deleteModule" prefWidth="95.0" text="Delete module" />
                  <Button fx:id="btn_saveModule" mnemonicParsing="false" onAction="#saveModule" prefWidth="95.0" text="Save module" />
               </children>
               <padding>
                  <Insets top="16.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
