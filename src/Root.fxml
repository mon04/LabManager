                                 </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </HBox>
                        <HBox layoutX="10.0" layoutY="68.0" prefHeight="160.0" prefWidth="200.0" spacing="4.0">
                           <children>
                              <VBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="2000.0" prefWidth="64.0" spacing="4.0">
                                 <children>
                                    <Label minWidth="-Infinity" prefHeight="17.0" text="Timed labs:" />
                                    <Button fx:id="btn_moduleGroupsEdit" mnemonicParsing="false" onAction="#openGroupsEditor" text="Edit" />
                                 </children>
                              </VBox>
                              <TableView fx:id="table_groups" focusTraversable="false" prefHeight="160.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                <columns>
                                  <TableColumn fx:id="tableColumn_groups_day" prefWidth="95.33330285549164" text="Day" />
                                  <TableColumn fx:id="tableColumn_groups_time" prefWidth="77.00000762939453" text="Time" />
                                    <TableColumn fx:id="tableColumn_groups_groupName" prefWidth="135.0" text="Group name" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </HBox>
                        <Separator prefWidth="200.0">
